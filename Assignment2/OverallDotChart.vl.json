{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "width": 1250,
    "height": 1000,
    "projection": {
        "type": "equalEarth",
        "center": [-97,38]
    },
    "layer": [
        {
            "data": {
                "url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/3_choropleth_map/js/ne_110m_admin_0_countries.topojson",
                "format": {
                    "type": "topojson",
                    "feature": "ne_110m_admin_0_countries"
                }
            },
            "mark": {
                "type": "geoshape",
                "fill": "#e5e8d3",
                "stroke": "#969696",
                "strokeWidth": 0.6
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/snag41/FIT3179/master/data/OverallDotChart.csv"
            },
            "mark": {
                "type": "circle",
                "color": "#039e5c"
            },
            "encoding": {
                "longitude": {
                    "field": "Longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "Latitude",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "name",
                        "type": "nominal",
                        "title": "Project Name"
                    },
                    {
                        "field": "Categories",
                        "type": "nominal",
                        "title": "Category"
                    },
                    {
                        "field": "Country",
                        "type": "nominal",
                        "title": "Country"
                    },
                    {
                        "field": "City",
                        "type": "nominal",
                        "title": "City"
                    }
                ]
            }
        }
    ],
    "config": {"background": "#62cff0",
        "padding": {"left": 0, "right": 0, "bottom": 0, "top": 0}}}
}